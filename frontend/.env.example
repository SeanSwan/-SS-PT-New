# SwanStudios Frontend Environment Variables
# Copy this file to .env.local and adjust as needed

# Development API URL
VITE_API_BASE_URL=http://localhost:5000

# Production API URL
# VITE_API_BASE_URL=https://swanstudios-api.onrender.com

# MCP Server Configuration
VITE_MCP_SERVER_URL=http://localhost:5000/api/mcp
VITE_WORKOUT_MCP_URL=http://localhost:8000
VITE_GAMIFICATION_MCP_URL=http://localhost:8002

# Environment
VITE_NODE_ENV=development

# App Configuration
VITE_APP_NAME=SwanStudios
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_AI_FEATURES=true
VITE_ENABLE_WORKOUT_GENERATOR=true
VITE_ENABLE_PROGRESS_ANALYSIS=true
VITE_ENABLE_NUTRITION_AI=true
VITE_ENABLE_EXERCISE_ALTERNATIVES=true