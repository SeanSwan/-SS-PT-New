{
  "name": "swanstudios-frontend",
  "version": "1.0.0",
  "description": "SwanStudios Gamified Fitness Social Ecosystem - Frontend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\" --names \"BACKEND,FRONTEND\" --prefix-colors \"blue,green\"",
    "start:frontend": "vite --host 0.0.0.0 --port 5173",
    "start:backend": "cd ../backend && npm start",
    "start:frontend-only": "vite --host 0.0.0.0 --port 5173",
    "verify": "node master-verification-suite.mjs",
    "verify:admin": "node verify-admin-dashboard-integration.mjs",
    "verify:api": "node verify-frontend-api-integration.mjs", 
    "verify:performance": "node verify-performance-accessibility.mjs",
    "verify:quick": "npm run verify:api",
    "test:full": "npm run verify",
    "health-check": "curl -f http://localhost:5173 && curl -f http://localhost:10000/api/health",
    "setup-verification": "npm install puppeteer axios lighthouse colors ws",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@reduxjs/toolkit": "^2.0.1",
    "@tanstack/react-query": "^5.8.4",
    "axios": "^1.6.2",
    "date-fns": "^3.0.6",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-big-calendar": "^1.8.5",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1",
    "styled-components": "^6.1.6",
    "socket.io-client": "^4.7.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.5",
    "puppeteer": "^21.0.0",
    "lighthouse": "^11.0.0",
    "colors": "^1.4.0",
    "ws": "^8.14.0"
  },
  "keywords": [
    "swanstudios",
    "fitness",
    "gamification",
    "social",
    "react",
    "vite",
    "universal-master-schedule"
  ],
  "author": "SwanStudios Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
