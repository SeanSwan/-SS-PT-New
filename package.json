    "@types/react-dom": "^19.0.3",
    "@types/react-helmet": "^6.1.11",
    "@types/styled-components": "^5.1.34",
    "concurrently": "^8.2.2",
    "eslint": "^9.20.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.66.0",
