{
  "name": "swanstudios",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start-frontend\" \"npm run start-backend\"",
    "start-frontend": "cd frontend && npm run dev",
    "start-backend": "cd backend && npm run dev",
    "start-with-db": "concurrently \"npm run start-frontend\" \"npm run start-backend-with-db\"",
    "start-backend-with-db": "cd backend && npm run dev:with-migrations",
    "db:migrate": "cd backend && npm run migrate",
    "db:seed": "cd backend && npm run seed",
    "db:setup": "cd backend && npm run db:setup",
    "db:reset": "cd backend && npm run db:reset",
    "db:reset-complete": "cd backend && npm run db:reset-complete",
    "fix-admin": "cd backend && npm run fix-admin",
    "reset-password": "cd backend && npm run reset-password",
    "emergency-fix": "cd backend && npm run emergency-fix",
    "verify-password": "cd backend && npm run verify-password",
    "force-create-admin": "cd backend && npm run force-create-admin",
    "fix-cart": "cd backend && npm run fix-cart",
    "migrate-cart": "cd backend && npm run migrate-cart",
    "seed-storefront": "cd backend && npm run seed-storefront",
    "install-all": "npm run install-frontend && npm run install-backend",
    "install-frontend": "cd frontend && npm install",
    "install-backend": "cd backend && npm install",
    "setup": "node -e \"console.log('Running setup script based on your OS...'); require('child_process').execSync(process.platform === 'win32' ? '.\\\\setup.bat' : './setup.sh', {stdio: 'inherit'});\"",
    "build": "cd frontend && npm install && npm run build"
  },
  "keywords": [],
