{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "server.mjs",
  "type": "module",
  "scripts": {
    "start": "node server.mjs",
    "dev": "nodemon server.mjs",
    "migrate": "npx sequelize-cli db:migrate --config config/config.js --migrations-path migrations --models-path models --env development",
    "migrate:undo": "npx sequelize-cli db:migrate:undo --config config/config.js --migrations-path migrations --models-path models --env development",
    "seed": "npx sequelize-cli db:seed:all --config config/config.js --seeders-path seeders --models-path models --env development",
    "seed:undo": "npx sequelize-cli db:seed:undo:all --config config/config.js --seeders-path seeders --models-path models --env development",
    "db:setup": "npm run migrate && npm run seed",
    "db:reset": "node scripts/reset-db-sequelize.mjs",
    "db:reset-complete": "node scripts/setup-complete.mjs",
    "create-admin": "node scripts/create-admin-user.mjs",
    "fix-admin": "node scripts/fix-admin-user.mjs",
    "reset-admin-password": "node scripts/reset-admin-password.mjs",
    "fix-cart": "node scripts/fix-cart-schema.mjs",
    "migrate-cart": "node scripts/migrate-cart-data.mjs",
    "seed-storefront": "node scripts/seed-storefront-items.mjs",
    "verify-admin": "node scripts/verify-admin.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
