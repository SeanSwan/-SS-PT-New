    "@sendgrid/mail": "^8.1.4",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "llama-node": "^0.1.6",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "mysql2": "^3.12.0",
    "nodemailer": "^6.10.0",
    "pg": "^8.13.3",
    "pg-hstore": "^2.3.4",
    "react-modal": "^3.16.3",
    "sequelize": "^6.37.5",
    "socket.io": "^4.8.1",
    "stripe": "^17.7.0",
    "swr": "^2.3.2",
    "twilio": "^5.4.5",
    "winston": "^3.17.0"
  },
